{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "node"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@atom/*": [
        "src/views/components/atoms/*"
      ],
      "@module/*": [
        "src/views/components/modules/*"
      ],
      "@template/*": [
        "src/views/components/templates/*"
      ],
      "@asset/*": [
        "src/views/assets/*"
      ],
      "@model/*": [
        "src/models/*"
      ],
      "@store/*": [
        "src/stores/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js"
  ]
}